<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonInserisci"
        android:layout_width="179dp"
        android:layout_height="48dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="207dp"
        android:onClick="InserisciStudente"
        android:text="@string/inserisci"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonVisualizza"
        android:layout_width="183dp"
        android:layout_height="46dp"
        android:layout_marginStart="114dp"
        android:layout_marginTop="58dp"
        android:onClick="VisualizzaStudente"
        android:text="@string/visualizza"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonInserisci" />
</androidx.constraintlayout.widget.ConstraintLayout>